@using CarInsurance.Models
@model List<Insuree>


<div class="row no-gutters" m-3>
   
    <!--// returning a List from Db/Model  -->
    <div class="card-body">
        <table class="table">
                
                <thead>
                    <tr>
                        <th>First Name</th>
                        <th>Last Name</</th>
                        <th>Email Address</th>
                        <th>Quote Total</th>
                    </tr>
                </thead>
                    
                <tbody>

                    @foreach (var insuree in Model)
                    {
                        <tr>
                            <td> @insuree.FirstName</td>
                            <td> @insuree.LastName</td>
                            <td> @insuree.EmailAddress</td>
                            <td> $@insuree.Quote</td>
                        </tr>
                    }

                </tbody>
         </table>
    </div>
    
</div>




